{
  "manifest_version": 2,
  "name": "GCP Header Color Changer",
  "description": "...",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://console.cloud.google.com/*"],
      "js": ["main.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "storage",
    "webNavigation"
  ]
}
